{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node10",
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "ignoreDeprecations": "6.0",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}